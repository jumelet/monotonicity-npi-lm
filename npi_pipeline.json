{
    "model": {
        "rnn_type": "ForwardLSTM",
        "state_dict": "/path_to_models"
    },
    "init_states": {
        "use_default": true
    },
    "tokenizer": {
        "notify_unk": false
    },
    "corpus": {
        "path": "data/monotonicity_probing.tsv",
        "sen_column": "sen",
        "labels_column": "labels",
        "header_from_first_line": true
    },
    "probe": {
        "save_dir": "results/",
        "verbose": 1,
        "lambda1": 0.005,
        "max_epochs": 10
    },
    "syntax": {
        "use_full_model_probs": true,
        "config": {
            "warstadt": {
                "path": "data/npi_data_all_environments.tsv"
            }
        }
    }
}
